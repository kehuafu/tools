<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/space_1"
    android:id="@+id/cl_item"
    android:background="@drawable/main_item">

    <TextView
        android:id="@+id/tv_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_46"
        android:gravity="center"
        android:text="@string/title"
        android:textColor="@color/color_b1_dark"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_go"
        android:layout_width="@dimen/space_10"
        android:layout_height="@dimen/space_20"
        android:layout_marginEnd="@dimen/space_15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_right_next_nor"
        tools:layout_editor_absoluteX="375dp"
        tools:layout_editor_absoluteY="13dp" />


</androidx.constraintlayout.widget.ConstraintLayout>